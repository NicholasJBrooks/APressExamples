{
  "ShortCircuitMiddleware": {
    "EnableBrowserShortCircuit": true
  },
  ///Logging will allow 6 levels of logging as follows Trace, Debug, Information, Warning, Error, Critical, None (Used to turn logging off)
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}